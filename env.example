# WandB API Key (실험 추적용)
WANDB_API_KEY=your_wandb_api_key_here

# CUDA 설정 (GPU 4개 사용)
CUDA_VISIBLE_DEVICES=0,1,2,3

# 모델 설정
MODEL_CACHE_DIR=/app/.cache/huggingface

# 로그 레벨
LOG_LEVEL=INFO

# 실험 설정
EXPERIMENT_NAME=conf_agg_experiment
EXPERIMENT_TAGS=math_reasoning,confidence_aggregation

# vLLM 설정 (GPU 4개 사용)
VLLM_TENSOR_PARALLEL_SIZE=4
VLLM_GPU_MEMORY_UTILIZATION=0.9

